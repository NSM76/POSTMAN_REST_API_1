	"item": [
		{
			"name": "Get API key",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Запись API-key в переменную auth_key\", function () {\r",
							"   var bodyData = JSON.parse(responseBody);\r",
							"   pm.expect(bodyData).to.have.all.keys('key');\r",
							"   pm.collectionVariables.set(\"auth_key\", bodyData.key);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "email",
						"value": "novboroda@mail.ru",
						"type": "text"
					},
					{
						"key": "password",
						"value": "1QAZxsw@",
						"type": "text"
					}
				],
				"url": "{{base_url}}/api/key"
			},
			"response": []
		},
		{
			"name": "Add Pet",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"    var bodyData = JSON.parse(responseBody);\r",
							"    pm.collectionVariables.set(\"petid\", bodyData.id);\r",
							"    pm.collectionVariables.set(\"name\", bodyData.name);\r",
							"\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "formdata",
					"formdata": [
						{
							"key": "name",
							"value": "{{name}}",
							"type": "text"
						},
						{
							"key": "animal_type",
							"value": "{{type}}",
							"type": "text"
						},
						{
							"key": "age",
							"value": "{{age}}",
							"type": "text"
						}
					]
				},
				"url": "{{base_url}}/api/create_pet_simple"
			},
			"response": []
		},
		{
			"name": "Check New Pet",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"first pet with new-created id\", function() {\r",
							"    var bodyData = JSON.parse(responseBody);\r",
							"    pm.expect(pm.collectionVariables.get(\"petid\")).to.eql(bodyData.pets[0].id);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{base_url}}/api/pets?filter=my_pets",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"pets"
					],
					"query": [
						{
							"key": "filter",
							"value": "my_pets"
						}
					]
				}
			},
			"response": []
		},
		{
			"name": "Delete Pet",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "DELETE",
				"header": [],
				"url": "{{base_url}}/api/pets/{{petid}}"
			},
			"response": []
		},
		{
			"name": "Check Dellete Pet",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"first pet has id not equal new-created id\", function() {\r",
							"    var bodyData = JSON.parse(responseBody);\r",
							"    pm.expect(pm.collectionVariables.get(\"petid\")).to.not.eql(bodyData.pets[0].id);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{base_url}}/api/pets?filter=my_pets",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"api",
						"pets"
					],
					"query": [
						{
							"key": "filter",
							"value": "my_pets"
						}
					]
				}
			},
			"response": []
		}
